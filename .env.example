# LogSentinel SDK Configuration

# Required: API key for authentication with monitoring server
LOGSENTINEL_API_KEY=your_api_key_here

# Required: Base URL of the monitoring server (no trailing slash)
LOGSENTINEL_BASE_URL=https://your-monitoring-server.com

# Optional: Enable debug mode with colorized console output (default: false)
LOGSENTINEL_DEBUG=true

# Optional: Number of logs to batch before sending (default: 5)
LOGSENTINEL_BATCH_SIZE=5
