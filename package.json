{
  "name": "logsentinel-nestjs",
  "version": "1.0.0",
  "description": "NestJS logging middleware that captures requests and responses, batches them asynchronously, and sends to remote server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nestjs",
    "logging",
    "middleware",
    "monitoring",
    "request-logging",
    "response-logging",
    "batch-processing"
  ],
  "author": "Eliakim Mwita",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "rxjs": "^7.0.0",
    "express": "^4.18.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "rxjs": "^7.8.1",
    "typescript": "^5.3.0"
  }
}
